GET    /       home.HomeController.index
GET    /employees    employee.EmployeeController.getAllEmployees
GET    /employees/:id    employee.EmployeeController.getEmployeeById(id: Long)
POST   /employees               employee.EmployeeController.create
PATCH  /employees/:id employee.EmployeeController.updateById(id: Long)
DELETE /employees/:id employee.EmployeeController.deleteById(id: Long)